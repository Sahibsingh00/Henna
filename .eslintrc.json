{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react/no-unescaped-entities": "off", // Disable unescaped entities warning
    "@typescript-eslint/no-unused-vars": "warn", // Change to "warn" to avoid errors on unused vars
    "jsx-a11y/alt-text": "warn", // Change to "warn" to avoid errors on missing alt text
    "react-hooks/exhaustive-deps": "warn", // Change to "warn" to avoid errors on useEffect dependencies
    "@typescript-eslint/no-explicit-any": "off", // Disable unexpected any rule
    "@typescript-eslint/no-empty-object-type": "off" // Disable empty object type rule
  }
}